    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "css-loader": "^0.25.0",
    "snabbdom": "^0.5.4",
    "snabbdom-jsx": "^0.3.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.5",
    "tslint": "^3.15.1",
    "tslint-config-standard": "^1.5.0",
    "typescript": "^2.0.6",
    "webpack": "^1.13.3",
    "webpack-bem-loader": "0.0.3",
    "webpack-dev-server": "^1.16.2"
