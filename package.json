{
  "name": "snabbdom-bem",
  "version": "0.0.0",
  "description": "Snabbdom for BEM methodology",
  "typings": "lib/index.d.ts",
  "main": "index.js",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "clean:lib": "rm -rf lib",
